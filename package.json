{
  "name": "anybrace",
  "displayName": "AnyBrace",
  "description": "Close the last open brace, bracket, or parenthesis",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/cpulvermacher/anybrace.git"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "anybrace.close",
        "title": "Close last open (, [, or {",
        "category": "AnyBrace"
      }
    ],
    "keybindings": [
      {
        "command": "anybrace.close",
        "key": "alt+]",
        "mac": "option+]",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build": "npx vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2"
  }
}